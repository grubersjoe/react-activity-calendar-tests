{
  "name": "remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev --port 3003",
    "start": "PORT=3003 remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "ssr-test": " rm -rf node_modules/.vite && npm run build && npm start",
    "dev-test": " rm -rf node_modules/.vite && npm run dev"
  },
  "dependencies": {
    "@remix-run/node": "^2.17.4",
    "@remix-run/react": "^2.17.4",
    "@remix-run/serve": "^2.17.4",
    "isbot": "^5.1.34",
    "react": "^19.1.1",
    "react-activity-calendar": "3.0.0-alpha.4",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@floating-ui/react": "^0.27.16",
    "@remix-run/dev": "^2.17.4",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.24",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-tsconfig-paths": "^6.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
